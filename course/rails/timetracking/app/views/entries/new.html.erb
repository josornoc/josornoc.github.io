<h1>New entry for project: </h1>
<h2>
	Project: <%=@project.name%> /// 
	Created At: <%=@project.created_at%> ///
	Id: <%=@project.id%>
</h2>
<%= form_for [@project, @entry] do |f|%>
	<%= f.label :hours %>
	<%= f.text_field :hours %>
	<br>
	<%= f.label :minutes %>
	<%= f.text_field :minutes %>
	<br>
	<%= f.label :date %>
	<%= f.text_field :date %>
	<br>
	<%= f.label :comments %>
	<%= f.text_field :comments %>
	<br>
	<%= f.submit "Save" %>
<% end %>