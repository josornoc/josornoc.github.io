<h1>Create new project: </h1>
<%= form_for [@project], :html => { :multipart => true } do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>
	<%= f.label :logo %>
	<br>
	<%= f.file_field :logo %>
	<br>
	<%= f.label :description %>
	<br>
	<%= f.text_area :description %>
	<br>
	<%= f.submit "Save" %>
<% end %>
<% if @project.errors.any? %>
	<ul>
		<% @project.errors.full_messages.each do |err_message| %>
			<li><%= err_message %></li>
		<% end %>
	</ul>
<% end %>