<h2>PROJECTS GO HERE:</h2>
<h3>here is a really nice var: <%=@yMama%></h3>
<ul>
	<%@projects.each do |project|%>
		<li>
			<h2><a href=<%="/projects/#{project.id}/entries"%> ><%=project.name%></a></h2>
			<p><%=project.description%></p>
			<p>Created in: <%=project.created_at%></p>
			<%if project.description != nil%>
				<p><%=project.description%></p>
			<%else%>
				<p>This description is really nothing...</p>
			<%end%>
		</li>
	<%end%>
</ul>